/**
	Overloads the -= operator, allowing you to subtract a vector from another.
	vector& vector::opSubAssign(const vector &in other);
	## Arguments:
		* const vector&in other: the vector to subtract.
*/

// Example:
void main() {
	vector v1(12, 10, 8);
	vector v2(5, 3, 1);
	v1 -= v2;
	alert("The first vector =", "(" + v1.x + ", " + v1.y + ", " + v1.z + ")");
}
