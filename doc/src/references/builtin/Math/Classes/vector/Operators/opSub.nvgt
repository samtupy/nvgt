/**
	Overloads the - operator, allowing you to subtract a vector from another.
	vector& vector::opSub(const vector &in other);
	## Arguments:
		* const vector&in other: the vector to subtract.
*/

// Example:
void main() {
	vector v1(12, 10, 8);
	vector v2(5, 3, 1);
	vector v3 = v1 - v2;
	alert("The third vector =", "(" + v3.x + ", " + v3.y + ", " + v3.z + ")");
}
